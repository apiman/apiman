  "scripts": {
    "apiman:build-docker": "mvn -f ../../../distro/wildfly/pom.xml install -Pdocker",
    "apiman:rebuild": "mvn -f ../../../parent/pom.xml clean install &&  mvn -f ../../../pom.xml clean install -DskipTests -Dmaven.javadoc.skip -T1C -B -Pdocker",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "start": "yarn run gulp",
    "ci": "cd ci && bash run-e2e.sh",
    "test:headless": "yarn cy:run",
    "watch": "webpack --watch --mode=development",
    "serve": "webpack serve --open --mode=development"
